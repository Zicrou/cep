<p id="notice"><%= notice %></p>

<h1>Typeservices</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Direction</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @typeservices.each do |typeservice| %>
      <tr>
        <td><%= typeservice.name %></td>
        <td><%= typeservice.direction %></td>
        <td><%= link_to 'Show', typeservice %></td>
        <td><%= link_to 'Edit', edit_typeservice_path(typeservice) %></td>
        <td><%= link_to 'Destroy', typeservice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Typeservice', new_typeservice_path %>
