<p id="notice"><%= notice %></p>

<h1>Expatries</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expatries.each do |expatry| %>
      <tr>
        <td><%= expatry.code %></td>
        <td><%= expatry.name %></td>
        <td><%= link_to 'Show', expatry %></td>
        <td><%= link_to 'Edit', edit_expatry_path(expatry) %></td>
        <td><%= link_to 'Destroy', expatry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expatry', new_expatry_path %>
